{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "CAMDAAnalytics"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorageFile2')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStorage2",
					"type": "LinkedServiceReference"
				},
				"type": "AzureBlobFSFile",
				"typeProperties": {
					"folderPath": "camda-tcga-brca"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/camda_tcga_brca_stanford')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "camdaUS",
					"type": "LinkedServiceReference"
				},
				"type": "FileShare",
				"typeProperties": {
					"fileName": "000837e3-cc9f-4226-ba38-44fa45140671_1.fastq.dsrc",
					"folderPath": "/Users/camda2019/CAMDA/BRCA"
				}
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline2')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "camda-tcga-brca-us",
						"type": "Copy",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 3,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "/Users/camda2019/CAMDA/BRCA/000837e3-cc9f-4226-ba38-44fa45140671_1.fastq.dsrc"
							},
							{
								"name": "Destination",
								"value": "camda-tcga-brca/"
							}
						],
						"typeProperties": {
							"source": {
								"type": "FileSystemSource",
								"recursive": true
							},
							"sink": {
								"type": "AzureBlobFSSink",
								"copyBehavior": "PreserveHierarchy"
							},
							"enableStaging": false,
							"parallelCopies": 8
						},
						"inputs": [
							{
								"referenceName": "camda_tcga_brca_stanford",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureDataLakeStorageFile2",
								"type": "DatasetReference"
							}
						]
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/camda_tcga_brca_stanford')]",
				"[concat(variables('factoryId'), '/datasets/AzureDataLakeStorageFile2')]"
			]
		}
	]
}